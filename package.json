{
  "name": "lib_template",
  "version": "1.0.0",
  "description": "template library",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --config build/webpack.dev.js",
    "lint": "eslint --ext .js src",
    "test": "npm run lint && jest",
    "build": "rimraf dist/* && webpack --config build/webpack.prod.js",
    "commit": "git add -A && git commit",
    "push": "git push && git push --tags",
    "pack": "npm run lint && npm test && npm run build && npm run commit",
    "bump:patch": "npm version patch && npm run push",
    "bump:minor": "npm version minor && npm run push",
    "release:patch": "npm run pack && npm run bump:patch && npm publish",
    "release:minor": "npm run pack && npm run bump:minor && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaomelo/package_template.git"
  },
  "keywords": [
    "keyords",
    "forlib"
  ],
  "author": "joaomelo",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/joaomelo/package_template/issues"
  },
  "homepage": "https://github.com/joaomelo/package_template#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "compression-webpack-plugin": "^3.1.0",
    "circular-dependency-plugin": "^5.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "rimraf": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  }
}